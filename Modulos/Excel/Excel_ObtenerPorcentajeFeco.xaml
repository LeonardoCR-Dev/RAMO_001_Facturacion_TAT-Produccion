    <ui:InvokeCode ContinueOnError="{x:Null}" Code="        ' Crear y configurar la tabla con las columnas necesarias.&#xD;&#xA;        Dim dt As New DataTable&#xD;&#xA;&#x9;&#x9;dt = dt_Feco&#xD;&#xA;        ' Contar el total de filas de la tabla.&#xD;&#xA;        Dim totalFilas As Integer = dt.Rows.Count&#xD;&#xA;        Dim filasValidas As Integer = 0&#xD;&#xA;&#xD;&#xA;        ' Si no hay filas, mostramos false y salimos.&#xD;&#xA;        If totalFilas = 0 Then&#xD;&#xA;            Console.WriteLine(&quot;False&quot;)&#xD;&#xA;            Console.ReadKey()&#xD;&#xA;            Return&#xD;&#xA;        End If&#xD;&#xA;&#xD;&#xA;        ' Recorrer cada fila para evaluar las columnas especificadas.&#xD;&#xA;        For Each fila As DataRow In dt.Rows&#xD;&#xA;            ' Convertir el valor de cada columna a cadena, considerando DBNull y espacios.&#xD;&#xA;            Dim valor2 As String = If(fila(2) Is DBNull.Value, &quot;&quot;, fila(2).ToString().Trim())&#xD;&#xA;            Dim valor12 As String = If(fila(12) Is DBNull.Value, &quot;&quot;, fila(12).ToString().Trim())&#xD;&#xA;            Dim valor17 As String = If(fila(17) Is DBNull.Value, &quot;&quot;, fila(17).ToString().Trim())&#xD;&#xA;&#xD;&#xA;            ' Se considera válida la fila si ninguna de las columnas está vacía.&#xD;&#xA;            If Not String.IsNullOrEmpty(valor2) AndAlso Not String.IsNullOrEmpty(valor12) AndAlso Not String.IsNullOrEmpty(valor17) Then&#xD;&#xA;                filasValidas += 1&#xD;&#xA;            End If&#xD;&#xA;        Next&#xD;&#xA;&#xD;&#xA;        ' Calcular el porcentaje de filas válidas.&#xD;&#xA;        Dim porcentajeValidas As Double = (filasValidas / totalFilas) * 100&#xD;&#xA;&#xD;&#xA;        ' Verificar si el porcentaje es mayor o igual al 90%.&#xD;&#xA;        Dim resultado As Boolean = porcentajeValidas &gt;= 90&#xD;&#xA;&#xD;&#xA;        ' Mostrar el resultado en consola.&#xD;&#xA;        Console.WriteLine(&quot;El resultado es: &quot; &amp; resultado.ToString())&#xD;&#xA;        Console.WriteLine(&quot;Filas válidas: &quot; &amp; filasValidas.ToString() &amp; &quot; de &quot; &amp; totalFilas.ToString() &amp; &quot; (&quot; &amp; porcentajeValidas.ToString(&quot;F2&quot;) &amp; &quot;%)&quot;)&#xD;&#xA;        ResultadoSalida=resultado&#xD;&#xA;&#x9;&#x9;Porcentaje=porcentajeValidas.ToString" DisplayName="Filtros en archivo FECO" sap:VirtualizedContainerService.HintSize="520,197" sap2010:WorkflowViewState.IdRef="InvokeCode_1">
